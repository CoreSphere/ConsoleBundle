{
    "name": "openclassrooms/console-bundle",
    "description": "This bundle allows you accessing the symfony3-4 console via your browser",
    "keywords": ["console","javascript","symfony"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laszlo Korte",
            "email": "coresphere@laszlokorte.de"
        },
        {
            "name": "Marmus Ullman",
            "email": "mail@markus-ullmann.de"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/console": "~3.4|~4.0",
        "symfony/dependency-injection": "~3.4|~4.0",
        "symfony/event-dispatcher": "~3.4|~4.0",
        "symfony/framework-bundle": "~3.4|~4.0",
        "symfony/yaml": "~3.4|~4.0",
        "symfony/http-kernel": "~3.4|~4.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "phpunit/phpunit": "~7.5",
        "symfony/finder": "~3.4|~4.0",
        "symfony/templating": "~3.4|~4.0"
    },
    "autoload": {
        "psr-4": {
            "CoreSphere\\ConsoleBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    }
}
