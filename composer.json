{
    "name": "coresphere/console-bundle",
    "description": "This bundle allows you accessing the symfony console via your browser",
    "keywords": ["console", "javascript", "symfony"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laszlo Korte",
            "email": "coresphere@laszlokorte.de"
        },
        {
            "name": "Marmus Ullman",
            "email": "mail@markus-ullmann.de"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/console": "^5.3|^6.0",
        "symfony/dependency-injection": "^5.3|^6.0",
        "symfony/event-dispatcher": "^5.3|^6.0",
        "symfony/framework-bundle": "^5.3|^6.0",
        "symfony/http-kernel": "^5.3|^6.0",
        "symfony/twig-bundle": "^5.3|^6.0",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4.0",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy-phpunit": "^2.0",
        "symfony/finder": "^5.3|^6.0",
        "symfony/templating": "^5.3|^6.0",
        "symfony/yaml": "^5.3|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CoreSphere\\ConsoleBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    }
}
